{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://close-web-staging.herokuapp.com/users/sign_in"
    },
    {
      "type": "verifyTextPresent",
      "text": "Login"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "user_email"
      },
      "text": "testy.close@gmail.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "user_password"
      },
      "text": "testtest"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "commit"
      }
    },
    {
      "type": "verifyElementValue",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='input-group']/input"
      },
      "value": ""
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "a[data-selenium='mainNav-settings']"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Settings"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Logout"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Sign Up"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}