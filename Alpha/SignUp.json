{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://alpha.close.co/users/sign_up"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "user_email"
      },
      "text": "testy"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "user_email"
      },
      "text": "testy.close+#{SecureRandom.hex(13)}@gmail.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "user_password"
      },
      "text": "testtest"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "commit"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Google"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='connect-approve']//button[.='Aceptar']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Continue"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "We are syncing your contacts."
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}