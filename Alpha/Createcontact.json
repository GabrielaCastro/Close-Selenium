{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://alpha.close.co/users/sign_in"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "user_email"
      },
      "text": "testy.close@gmail.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "user_password"
      },
      "text": "testtest"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "commit"
      }
    },
    {
      "type": "verifyElementValue",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='input-group']/input"
      },
      "value": ""
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "i.fa.icon-statusplus"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Create a Contact"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='modal-content']/div/form/div[1]/input"
      },
      "text": "Name"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='modal-content']/div/form/div[2]/input"
      },
      "text": "email@test.com"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='modal-content']//button[.='Submit']"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Name"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}