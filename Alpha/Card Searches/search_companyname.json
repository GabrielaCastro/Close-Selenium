{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://alpha.close.co/users/sign_in"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "user_email"
      },
      "text": "testy.close@gmail.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "user_password"
      },
      "text": "testtest"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "commit"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='input-group']/input"
      },
      "text": "New Futuro"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "contacts_queryContacts"
      }
    },
    {
      "type": "waitForTextPresent",
      "text": "Company name"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}